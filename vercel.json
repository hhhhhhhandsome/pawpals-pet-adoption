{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/pets/:id",
            "destination": "/api/pets/[id]"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}